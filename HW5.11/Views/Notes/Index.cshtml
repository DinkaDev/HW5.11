@model List<Note>

    <h2>Notes</h2>

    @if (Model.Count>0){
        <table class="table">
            <thead>
                <tr>
                    <th>Title</th>
                    <th>Content</th>
                </tr>
            </thead>
            <tbody>
            @foreach(var note in Model)
            {
                <tr>
                    <th>@note.Title</th>
                    <th>@note.Content</th>
                </tr>
            }
            </tbody>
        </table>
    }
    else{
        <p>No notes found.</p>
    }